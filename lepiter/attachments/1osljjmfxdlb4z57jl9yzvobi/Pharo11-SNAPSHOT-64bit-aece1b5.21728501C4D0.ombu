			#name : #renderLscAddingInstance,
			#protocol : #'examples - ui',
			#sourceCode : 'renderLscAddingInstance\r\t"Check that the UI element displays the model\'s characteristics"\r\r\t<gtExample>\r\t| lsc uiElement instancesCanvasElement initialInstancesSize instanceAdded instanceAddedPostion instanceAdddElement |\r\tlsc := self renderNewLsc.\r\tinitialInstancesSize := lsc instancesSize.\r\tuiElement := lsc asElement.\r\t\r\t"Add an instance to the model"\r\tinstanceAdded := GtLscInstanceModelExamples new newLscInstance\r\t\tname: \'Instance added\';\r\t\ttemperature: GtLscColdTemperature;\r\t\tyourself.\r\tlsc addInstance: instanceAdded.\r\t\r\tinstanceAddedPostion := initialInstancesSize + 1.\r\t\r\t"Check the instance in the view"\r\tinstancesCanvasElement := (uiElement childNamed: GtLscConstants instancesCanvasId).\r\tself assert: instancesCanvasElement isNotNil.\r\tself assert: instancesCanvasElement childrenCount equals: instanceAddedPostion.\r\t\r\tinstanceAdddElement := (instancesCanvasElement childAt: instanceAddedPostion).\r\t"Check element id"\r\tself assert: instanceAdddElement id equals: (GtLscConstants instanceElementIdTemplate format: {instanceAdded id asString}) asBlocElementId.\r\t"Check name"\r\tself assert: (instanceAdddElement childNamed: #nameElement) text asString equals: instanceAdded name.\r\t"Check locationLine border"\r\tself assert: instanceAdddElement border paint equals: GtLscConstants coldColor asBlPaint.\r\r\t^ lsc',			#stamp : 'GlamorousAuthor 4/8/2025 10:13',
